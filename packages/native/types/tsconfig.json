{
  "compilerOptions": {
    "baseUrl": "../",
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "lib": ["es6", "dom"],
    "module": "commonjs",
    "noEmit": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strict": true,
    "target": "es5",
    "typeRoots": ["../"],
    "types": [],
    "skipLibCheck": true
  },
  "include": ["./*.ts", "./*.tsx"]
}
